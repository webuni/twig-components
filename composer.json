{
    "name": "webuni/twig-components",
    "description": "Define reusable components in Twig/Twital as in Vue/React style",
    "type": "library",
    "require": {
        "php": "^7.2|^8.0",
        "ext-dom": "*",
        "goetas/twital": "^1.0",
        "twig/twig": "^1.44|^2.14|^3.1"
    },
    "require-dev": {
        "symfony/http-kernel": "^3.4|^4.4|^5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Haso≈à",
            "email": "martin.hason@gmail.com"
        },
        {
            "name": "Webuni s.r.o.",
            "homepage": "https://www.webuni.cz"
        }
    ],
    "autoload": {
        "psr-4": {
            "Webuni\\TwigComponents\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webuni\\TwigComponents\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": "./run cs",
        "cs-check": "./run cs --dry-run",
        "analyse": "./run analyse",
        "tests": "./run tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
